{
    "git.terminalAuthentication": true,
    "telemetry.enableTelemetry": false,
    "search.followSymlinks": true,
    "problems.showUnused": true,
    "comments.openView": "file",
    "update.mode": "none",
    "breadcrumbs.enabled": true,
    "extensions.autoCheckUpdates": true,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableErrorTelemetry": false,
    "files.hotExit": "off",
    "files.saveConflictResolution": "askUser",
    "files.autoSave": "off",
    "files.autoSaveDelay": 12000,
    "editor.lineHeight": 18,
    "editor.wordWrap": "on",
    "editor.tabSize": 4,
    "editor.renderWhitespace": "trailing",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.detectIndentation": true,
    "editor.parameterHints": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true
    },
    "editor.snippetSuggestions": "bottom",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    }, //PERFORMANCE=false
    "editor.semanticTokenColorCustomizations": {
        "enabled": false //PERFORMANCE=false
    },
    "editor.fontSize": 17,
    "editor.fontLigatures": true,
    "editor.fontFamily": "Consolas, Lucida Console, DejaVu Sans Mono, Liberation Mono, Courier, Consolas, Menlo, Monaco, monospace, Arial",
    "workbench.tree.indent": 8,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.startupEditor": "none",
    "workbench.sideBar.location": "left",
    "workbench.panel.defaultLocation": "right",
    "workbench.panel.opensMaximized": "never",
    "workbench.colorTheme": "Copilot Theme",
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.showExitAlert": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.defaultProfile.osx": "bash",
    "terminal.integrated.defaultProfile.windows": "bash",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "liveServer.settings.host": "0.0.0.0",
    "tabnine.experimentalAutoImports": true,
    "tabnine.useProxySupport": false
}