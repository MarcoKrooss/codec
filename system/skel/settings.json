{
    "git.terminalAuthentication": true,
    "git.autoStash": true,
    "git.repositoryScanIgnoredFolders": [
        "node_modules",
        ".codec"
    ],
    "telemetry.enableTelemetry": false,
    "search.followSymlinks": true,
    "problems.showUnused": true,
    "comments.openView": "file",
    "update.mode": "none",
    "breadcrumbs.enabled": true,
    "extensions.autoCheckUpdates": true,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableErrorTelemetry": false,
    "files.hotExit": "off",
    "files.saveConflictResolution": "askUser",
    "files.autoSaveDelay": 12000,
    "editor.lineHeight": 18,
    "editor.wordWrap": "on",
    "editor.tabSize": 4,
    "editor.renderWhitespace": "trailing",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.detectIndentation": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.snippetSuggestions": "bottom",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    }, //PERFORMANCE=false
    "editor.semanticTokenColorCustomizations": {
        "enabled": false //PERFORMANCE=false
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "Menlo, Monaco, Consolas, 'Courier New', Consolas, Lucida Console, 'Droid Sans', sans-serif, Arial",
    "workbench.tree.indent": 8,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.startupEditor": "none",
    "workbench.panel.defaultLocation": "right",
    "workbench.panel.opensMaximized": "never",
    "workbench.colorTheme": "Default Dark Modern",
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.showExitAlert": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.defaultProfile.osx": "bash",
    "terminal.integrated.defaultProfile.windows": "bash",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "tabnine.experimentalAutoImports": true,
    "tabnine.useProxySupport": false,
    "markdown.extension.toc.levels": "1..3",
    "remote.autoForwardPorts": false,
    "liveshare.anonymousGuestApproval": "accept",
    "hediet.vscode-drawio.resizeImages": null,
    "vscode_custom_css.imports": [
        "file:///codec/mounts/vscode/extensions/coltwillcox.synthwave-x-fluoromachine-contrast-1.0.2/synthwave-x-fluoromachine.css",
        "file:///codec/mounts/vscode/extensions/coltwillcox.synthwave-x-fluoromachine-contrast-1.0.2/epic-80s-transitions.css",
        "file:///codec/mounts/vscode/extensions/coltwillcox.synthwave-x-fluoromachine-contrast-1.0.2/logo.css"
    ],
    "workbench.colorCustomizations": {
        "minimap.background": "#00000000",
        "tab.inactiveBackground": "#0c0e14"
    },
    "keyboard.dispatch": "keyCode",
    "vsicons.dontShowNewVersionMessage": true,
    "hediet.vscode-drawio.zoomFactor": 2,
    "editor.mouseWheelZoom": true,
    "keyboard.layout": "de",
    "editor.fontSize": 14,
    "sshfs.configs": [],
    "sshfs.configpaths": [
        "/codec/.codec/sshfs.jsonc"
    ],
    "workbench.activityBar.location": "top",
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "never",
    "window.menuBarVisibility": "hidden",
    "workbench.statusBar.visible": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "cSpell.userWords": [
        "debconf",
        "ipaddress",
        "majo",
        "Millis",
        "noble",
        "noblemajo",
        "vrrp"
    ],
    "workbench.iconTheme": "vscode-great-icons",
}