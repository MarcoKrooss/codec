#!/bin/bash

export DEBIAN_FRONTEND=noninteractive

sudo -E apt-get update
sudo -E apt-get full-upgrade -y
sudo -E apt-get autoremove -y
sudo -E apt-get clean
sudo -E rm -rf /tmp/*

