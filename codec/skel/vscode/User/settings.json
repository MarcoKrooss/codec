{
    "window.zoomLevel": 1.4,
    "workbench.startupEditor": "none",
    "files.autoSave": "off",
    "workbench.colorTheme": "Ubuntu Color VSCode Dark Highlight",
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "left",
